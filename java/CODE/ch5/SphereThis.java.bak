class Sphere {
//instance variables
  double x;
  double y;
  double z;
  double radius;

//default constructor
  Sphere() {
    this(0, 0, 0, 1);
  }

//parameterized constructor
  Sphere(double radius) {
    this(0, 0, 0, radius);
  }

  Sphere(double x, double y, double z, double radius) {
    this.x = x;
    this.y = y;
    this.z = z;
    this.radius = radius;
  }
}

