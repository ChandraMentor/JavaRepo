class  SwitchDemo1{
public static void  main(String args[]) {
  int i=9;
    switch(i) {
       default:
	    System.out.println("No Matching Expression");
	   break;
	  case 1:
        System.out.println("one");
        break;
      case 2:
        System.out.println("two");
        break;
      case 19:
     System.out.println("nine again");
        break;
      case 4:
        System.out.println("four");
        break;
    }
  }
}