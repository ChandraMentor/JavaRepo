<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="address1" class="com.ci.Address">
<constructor-arg value="21,LB Nagar"></constructor-arg>
<constructor-arg value="Hyd"></constructor-arg>
<constructor-arg value="UT"></constructor-arg>
<constructor-arg value="India"></constructor-arg>
</bean>

<bean id="e1" class="com.ci.Employee">
<constructor-arg value="101"></constructor-arg>
<constructor-arg  value="Saba"></constructor-arg>
<constructor-arg ref="address1"></constructor-arg>
</bean>
</beans>